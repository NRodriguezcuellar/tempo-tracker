{
  "name": "@nicorodri/tempo-cli",
  "version": "0.0.0-experimental-5e8b5c1",
  "description": "CLI tool for tracking time spent on git branches and syncing with Tempo",
  "main": "dist/index.js",
  "bin": {
    "tempo": "dist/index.js"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build --target=node --minify --outfile=dist/index.js ./src/main.ts",
    "dev": "bun --watch src/main.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nicorodri/tempo-core": "0.0.0-experimental-5e8b5c1",
    "@nicorodri/tempo-daemon": "0.0.0-experimental-5e8b5c1",
    "axios": "1.8.3",
    "chalk": "5.4.1",
    "cli-table3": "0.6.5",
    "commander": "13.1.0",
    "inquirer": "12.5.0",
    "zod": "3.24.2"
  }
}
